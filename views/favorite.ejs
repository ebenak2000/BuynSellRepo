<!-- favorite.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User Favorites</title>
</head>
<body>
    <h1>User Favorites</h1>
    <ul>
        <% data.forEach(item => { %>
            <li><%= item.title %> - <%= item.price %></li>
        <% }) %>
    </ul>
</body>
</html>
